# API Configuration
API_URL=http://localhost:3000 # Base URL for the API
TEST_PORT=8888                # Port for test server (used in SSRF tests)

# Test User Credentials
TEST_USERNAME=test    # Username for test account
TEST_PASSWORD=test123 # Password for test account

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-2024 # Secret key for JWT tokens

# Test Client Data
TEST_CLIENT_NAME=Test Client
TEST_CLIENT_CITY=Test City
TEST_CLIENT_ADDRESS=Test Address
TEST_CLIENT_PHONE=1234567890
TEST_CLIENT_EMAIL=test@example.com

# Test Seller Data
TEST_SELLER_NAME=Test Seller
TEST_SELLER_CITY=Test City
TEST_SELLER_ADDRESS=Test Address
TEST_SELLER_PHONE=1234567890
TEST_SELLER_EMAIL=test@example.com

# SSRF Test Configuration
# Comma-separated list of protocols to test
SSRF_TEST_PROTOCOLS=file:///etc/passwd,gopher://localhost:3000/_GET,dict://localhost:11211/stats

# Comma-separated list of IP ranges to test
SSRF_TEST_IP_RANGES=127.0.0.1,192.168.0.1,10.0.0.1,172.16.0.1,localhost
